        <fullName>Require_Recipient_for_Swag_Packs</fullName>
        <active>true</active>
        <description>Requires Recipient for Swag Packs or Exec Letter</description>
        <errorConditionFormula>AND(
ISBLANK(Desired_Recipient__c),
OR(
CONTAINS(TEXT(Content_Type__c),&quot;Swag Pack&quot;),
CONTAINS(TEXT(Content_Type__c),&quot;Executve Letter&quot;)
)
)</errorConditionFormula>
