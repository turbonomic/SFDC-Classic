    <validationRules>
        <fullName>Eval_Date_Validations</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Updated_License_End_Date__c),
NOT(ISBLANK(Updated_License_End_Date__c)),
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
NOT(CONTAINS($UserRole.Name,&quot;Marketing&quot;)),
OR(TEXT(License_Type__c)=&quot;VHM&quot;,TEXT(License_Type__c)=&quot;Freemium&quot;),
Updated_License_End_Date__c - TODAY() &gt; 90,
Updated_License_End_Date__c &gt; TODAY()+2,
NOT(INCLUDES(License_Update_Use_Cases__c,&quot;Downgrading to VHM&quot;)),
NOT(INCLUDES(License_Update_Use_Cases__c,&quot;NFR License&quot;)),
NOT(INCLUDES(License_Update_Use_Cases__c,&quot;Internal Use&quot;)))</errorConditionFormula>
        <errorDisplayField>Updated_License_End_Date__c</errorDisplayField>
        <errorMessage>Eval license updates may not exceed 90 days from today or fall within 2 days of today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Eval_Downgrade_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
ISCHANGED(Updated_License_End_Date__c),
OR(Updated_License_End_Date__c - TODAY() &gt; 400, Updated_License_End_Date__c &lt; TODAY()+2),
TEXT(License_Type__c)=&quot;Eval&quot;,
INCLUDES(License_Update_Use_Cases__c,&quot;Downgrading to VHM&quot;))</errorConditionFormula>
        <errorDisplayField>Updated_License_End_Date__c</errorDisplayField>
        <errorMessage>When downgrading to VHM, you may not set the updated license end date more than 400 days or within 2 days of today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Eval_Downgrade_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
NOT(ISNULL(Updated_Features__c)),
TEXT(License_Type__c)=&quot;Eval&quot;,
INCLUDES(License_Update_Use_Cases__c,&quot;Downgrading to VHM&quot;))</errorConditionFormula>
        <errorMessage>You may not have updated features if downgrading from VHM in your use cases. Please empty the Updated Features field or remove the Downgrading to VHM use case in the License Update Use Cases field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finance_Only_Primary_Com_Key_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Primary_License_Key__c),
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;)</errorConditionFormula>
        <errorMessage>Only finance can denote the primary license key</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finance_Only_Primary_Com_Key_Change2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(License_Type__c),
TEXT(License_Type__c)=&quot;Commercial&quot;,
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;)</errorConditionFormula>
        <errorDisplayField>License_Type__c</errorDisplayField>
        <errorMessage>Only finance can denote a commercial license type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Opportunity_And_Account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),
TEXT(License_Type__c) = &quot;Commercial&quot;,
ISBLANK(Opportunity__c),
ISBLANK(Account__c),
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
$User.Full_Name__c&lt;&gt;&quot;Gregory Dunn&quot;)</errorConditionFormula>
        <errorMessage>Please make sure this License has an Opportunity and Account associated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Other_Reason_for_Other_Use_Case</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
OR(
ISBLANK(Use_Case_Other_Reason__c),
INCLUDES(License_Creation_Use_Cases__c,&quot;Other&quot;)),
OR(
ISBLANK(Other_Reason__c),
INCLUDES(License_Update_Use_Cases__c,&quot;Other&quot;)))</errorConditionFormula>
        <errorMessage>Please fill in the Other Reason field in order to choose Other as a License Creation/Update Use Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Use_Cases</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$User.Full_Name__c&lt;&gt;&quot;Gregory Dunn&quot;,
ISNEW(),
OR(
INCLUDES(License_Creation_Use_Cases__c,&quot;Form Download&quot;),
INCLUDES(License_Creation_Use_Cases__c,&quot;Commercial License Creation&quot;)))</errorConditionFormula>
        <errorDisplayField>License_Creation_Use_Cases__c</errorDisplayField>
        <errorMessage>Only Finance/Admins can choose Form Download or Commercial License Creation as a use case when creating licenses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Upgrade_from_VHM_to_Eval_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
INCLUDES(License_Update_Use_Cases__c,&quot;Upgrading VHM to Eval&quot;), 
TEXT(License_Type__c)&lt;&gt;&quot;VHM&quot;)</errorConditionFormula>
        <errorMessage>You cannot upgrade a non-VHM license to an Eval license.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VHM_Date_Validations</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Updated_License_End_Date__c),
NOT(ISBLANK(Updated_License_End_Date__c)),
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
NOT(CONTAINS($UserRole.Name,&quot;Marketing&quot;)),
OR(TEXT(License_Type__c)=&quot;VHM&quot;,TEXT(License_Type__c)=&quot;Freemium&quot;),
Updated_License_End_Date__c - TODAY() &gt; 370,
Updated_License_End_Date__c &gt; TODAY()+2,
NOT(INCLUDES(License_Update_Use_Cases__c,&quot;Upgrading VHM to Eval&quot;)))</errorConditionFormula>
        <errorDisplayField>Updated_License_End_Date__c</errorDisplayField>
        <errorMessage>VHM/Freemium updates may not exceed 370 days from today or within 2 days of today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VHM_Upgrade_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
OR(TEXT(License_Type__c)=&quot;VHM&quot;,TEXT(License_Type__c)=&quot;Freemium&quot;),
INCLUDES(License_Update_Use_Cases__c,&quot;Upgrading VHM to Eval&quot;),
NOT(INCLUDES(License_Update_Use_Cases__c,&quot;NFR License&quot;)),
OR(Updated_License_End_Date__c - TODAY() &gt; 90, Updated_License_End_Date__c &lt; TODAY()+2)
)</errorConditionFormula>
        <errorDisplayField>Updated_License_End_Date__c</errorDisplayField>
        <errorMessage>When Upgrading from VHM to Eval, the updated license end date may not exceed 90 days or fall within 2 days of today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VHM_no_new_features</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
TEXT(License_Type__c)=&quot;VHM&quot;, 
NOT(ISNULL(Updated_Features__c)), 
NOT(INCLUDES(License_Update_Use_Cases__c,&quot;Upgrading VHM to Eval&quot;)))</errorConditionFormula>
        <errorDisplayField>Updated_Features__c</errorDisplayField>
        <errorMessage>You may not add features to VHM without upgrading to an Eval License.</errorMessage>
