IF( 
  Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c &gt;= 1000, 
  RIGHT(TEXT(FLOOR(Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c / 1000)), 3) &amp; &quot;,&quot;, 
