RIGHT(TEXT(FLOOR(Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c)), 3) &amp; &quot;.&quot; &amp; 
IF( 
  MOD(Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c , 1) * 100 &lt; 10, 
  &quot;0&quot; &amp; TEXT(ROUND(MOD(Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c , 1), 2) * 100), 
  TEXT(MIN(ROUND(MOD(Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c , 1), 2) * 100, 99)) 
