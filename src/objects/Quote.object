&quot;UK Pounds&quot;,&quot;£&quot;, 
&quot;Euros&quot;,&quot;€&quot;,
&quot;Canadian Dollars&quot;,&quot;C$&quot;, 
&quot;&quot;)&amp; 

IF( 
  Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c &gt;= 1000000, 
  TEXT(FLOOR(Total_List_Price_Currency__c*Exchange_Rate_for_Calcs__c / 1000000)) &amp; &quot;,&quot;, 
