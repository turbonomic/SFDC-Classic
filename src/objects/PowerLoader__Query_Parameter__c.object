    <validationRules>
        <fullName>PowerLoader__Display_type_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ispickval(PowerLoader__Display_Type__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>PowerLoader__Display_Type__c</errorDisplayField>
        <errorMessage>Display type is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PowerLoader__List_Source_Type_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ispickval(PowerLoader__List_Source_Type__c, &apos;&apos;) &amp;&amp; (
ispickval(PowerLoader__Display_Type__c, &apos;Radio&apos;) ||
ispickval(PowerLoader__Display_Type__c, &apos;Single Picklist&apos;) ||
ispickval(PowerLoader__Display_Type__c, &apos;Multi Picklist&apos;)
)</errorConditionFormula>
        <errorMessage>List source type is required for selected Display Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PowerLoader__List_source_required_for_SOQL</fullName>
        <active>true</active>
        <errorConditionFormula>(isblank(PowerLoader__List_Source__c) || isblank( PowerLoader__List_Value_Field_API_Name__c ) || isblank(PowerLoader__List_Label_Field_API_Name__c)) 
&amp;&amp; ispickval(PowerLoader__List_Source_Type__c, &apos;SOQL&apos;)</errorConditionFormula>
        <errorMessage>List source (SOQL select statement), label field, and value field are required for List Source Type SOQL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PowerLoader__Lookup_Object_API_Name_required</fullName>
        <active>true</active>
        <errorConditionFormula>isblank( PowerLoader__Object_API_Name__c ) &amp;&amp; ispickval(PowerLoader__Display_Type__c , &apos;Lookup&apos;)</errorConditionFormula>
        <errorDisplayField>PowerLoader__Object_API_Name__c</errorDisplayField>
        <errorMessage>Lookup Object API Name is required when Display Type is Lookup</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PowerLoader__Object_API_required_for_Field_Definition</fullName>
        <active>true</active>
        <errorConditionFormula>(
    isblank( PowerLoader__Object_API_Name__c ) ||
    isblank( PowerLoader__List_Value_Field_API_Name__c )
)
&amp;&amp; ispickval( PowerLoader__List_Source_Type__c , &apos;Field Definition&apos;)</errorConditionFormula>
        <errorMessage>Object API Name and List Value Field API Name are required for List Source Type Field Definition</errorMessage>
    </validationRules>
