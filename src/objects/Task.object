    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Cannot_Complete_Own_Demos</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Status),
TEXT(Status)=&quot;Completed&quot;,
TEXT(Type)=&quot;Demo&quot;,
$User.Full_Name__c&lt;&gt;Assigned_To__c,
NOT(CONTAINS($UserRole.Name,&quot;Enterprise&quot;)),
OR(
CONTAINS($UserRole.Name,&quot;Sales Development Rep&quot;),
CONTAINS($UserRole.Name,&quot;Business Development Rep&quot;)))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>BDR/SDRs are not allowed to complete demos assigned to an AE. Please have the AE or SE complete the demo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_change_call_task_into_demo</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(PRIORVALUE(Type),&quot;Call&quot;),
ISPICKVAL(Type,&quot;Demo&quot;),
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;
)</errorConditionFormula>
        <errorDisplayField>Type</errorDisplayField>
        <errorMessage>Cannot convert a call task into a demo task. Please create a new demo task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Include_env_size_on_demos</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
TEXT(Type)=&quot;Demo&quot;, 
OR(
  AND(
    OR(ISBLANK(Virtual_Machines_VMs__c),ISBLANK(Hosts__c)),
    OR(YEAR(ActivityDate)&gt;= 2014,ISBLANK(ActivityDate))),
  AND(
    OR(ISBLANK(Virtual_Machines_VMs__c),ISBLANK(Hosts__c),
    AND(ISBLANK(Cores__c),ISPICKVAL(Status,&quot;Completed&quot;))
    ),
    OR(ActivityDate&gt;=DATE(2016,08,14),ISBLANK(ActivityDate)))),
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;,
$User.FirstName&lt;&gt;&quot;Shmuel&quot;,
$UserRole.Name&lt;&gt;&quot;Director of Support&quot;,
$UserRole.Name&lt;&gt;&quot;Director of Customer Support&quot;)</errorConditionFormula>
        <errorDisplayField>Virtual_Machines_VMs__c</errorDisplayField>
        <errorMessage>VMs and hosts (host field is below this one) are required for demos. Cores are also required for completed demos. CPU socket count (two fields down) is not required but strongly recommended for tracking purposes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__cannot_edit_Priority_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <description>Validation Rule not in use</description>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>Priority</errorDisplayField>
        <errorMessage>You cannot edit the Priority field on LInkedin Tasks</errorMessage>
    </validationRules>
    <validationRules>
