        <fullName>Created_Period_for_TVs__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Created_Date_Adj__c =TODAY(),1, 
IF(Created_Date_Adj__c &gt;=TODAY()-MOD(TODAY()-DATE(1900, 1, 7), 7),2, 
IF(Created_Date_Adj__c &gt;=DATE(YEAR(TODAY()),MONTH(TODAY()),1),3, 
4)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Created Period for TVs</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Created_Quarter__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND( Created_Date_Adj__c &gt;= DATE(2014,1,1), Created_Date_Adj__c &lt; DATE(2014,4,1)),&quot;FY14Q1&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2014,4,1), Created_Date_Adj__c &lt; DATE(2014,7,1)),&quot;FY14Q2&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2014,7,1), Created_Date_Adj__c &lt; DATE(2014,10,1)),&quot;FY14Q3&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2014,10,1), Created_Date_Adj__c &lt; DATE(2015,1,1)),&quot;FY14Q4&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2015,1,1), Created_Date_Adj__c &lt; DATE(2015,4,1)),&quot;FY15Q1&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2015,4,1), Created_Date_Adj__c &lt; DATE(2015,7,1)),&quot;FY15Q2&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2015,7,1), Created_Date_Adj__c &lt; DATE(2015,10,1)),&quot;FY15Q3&quot;,
IF( AND( Created_Date_Adj__c &gt;= DATE(2015,10,1), Created_Date_Adj__c &lt; DATE(2016,1,1)),&quot;FY15Q4&quot;,
&quot;N/A&quot;))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Created Quarter</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
