    <fields>
        <fullName>Brain_Lookup_split__c</fullName>
        <externalId>false</externalId>
        <formula>IF(TEXT(Opportunity.Type)=&quot;Maintenance Renewal&quot;,&quot;Maintenance Renewals&quot;,
  IF(AND(TEXT(Opportunity.Type)=&quot;Subscription Renewal&quot;,Opportunity.CloseDate&lt;DATE(2016,1,1),OR(Opportunity.Account.Name=&quot;Computer Sciences Corporation&quot;,Opportunity.Account.Name=&quot;CSC&quot;,CONTAINS(Opportunity.Account.Name,&quot;CSC [&quot;))),&quot;CSC&quot;, 
    IF(Split_Owner_Team__c=&quot;Finance&quot;,&quot;Finance&quot;, 
      IF(AND(SplitOwner.Full_Name__c=&quot;Dominic DiMeco&quot;,Opportunity.CloseDate&lt;=DATEVALUE(&quot;2015-3-31&quot;)),&quot;Michael Killoran&quot;, 
        IF(Split_Owner_Team__c=&quot;Federal&quot;,SplitOwner.Full_Name__c, 
          IF(CONTAINS(Split_Owner_Team__c,&quot;Enterprise&quot;),
            IF(CONTAINS(SplitOwner.UserRole.Name,&quot;Enterprise Rep&quot;),SplitOwner.Full_Name__c, 
              CASE(SplitOwner.Full_Name__c, 
              &quot;Peter Jadovich&quot;,&quot;Eric Winslow&quot;, 
              SplitOwner.Full_Name__c)), 
          IF(Split_Owner_Team__c=&quot;Global Accounts US&quot;,IF(Opportunity.CloseDate&lt;DATEVALUE(&quot;2015-7-1&quot;),&quot;Chris Reisig&quot;,SplitOwner.Full_Name__c), 
            IF(Split_Owner_Team__c=&quot;Global Accounts International&quot;,IF(Opportunity.CloseDate&lt;DATEVALUE(&quot;2015-7-1&quot;),&quot;Andrew Mallaband&quot;,SplitOwner.Full_Name__c), 
              Split_Owner_Team__c
            )
          )
          )
        )
      )
    )
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Brain Lookup (split)</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
