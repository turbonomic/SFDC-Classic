&quot;Upside&quot;,0.4*MIN(75000,Opportunity.Amount*SplitPercentage), 
&quot;Strong Upside&quot;,0.6*MIN(75000,Opportunity.Amount*SplitPercentage), 
&quot;Commit&quot;,0.95*MIN(75000,Opportunity.Amount*SplitPercentage), 
&quot;PO received&quot;,Opportunity.Amount*SplitPercentage, 
&quot;Closed Won&quot;,Opportunity.Amount*SplitPercentage, 
0), 

CASE(Opportunity.StageName,
&quot;Pipeline&quot;,0.15* MIN(250000,Opportunity.Amount*SplitPercentage), 
&quot;Upside&quot;,0.5*MIN(250000,Opportunity.Amount*SplitPercentage), 
