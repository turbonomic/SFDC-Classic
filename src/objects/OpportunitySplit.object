    <fields>
        <fullName>Forecasted_Split_Bookings_Quotas__c</fullName>
        <description>For use in the Turbo Weekly Dash</description>
        <externalId>false</externalId>
        <formula>IF(TEXT(Opportunity.Type)=&quot;Maintenance Renewal&quot;,
CASE(Opportunity.StageName, 
&quot;Renewal&quot;,0.8, 
&quot;Identify&quot;,0.2, 
&quot;Pipeline&quot;,0.8, 
&quot;Commit&quot;,0.95, 
&quot;PO received&quot;,1, 
&quot;Closed Won&quot;,1, 
0)*Opportunity.Amount*SplitPercentage, 

IF(CONTAINS(Split_Owner_Team_Static_Group_Buckets__c,&quot;Velocity&quot;), 
CASE(Opportunity.Probability, 
0.25,0.25*MIN(100000,Opportunity.Amount*SplitPercentage), 
0.5,0.5*MIN(100000,Opportunity.Amount*SplitPercentage), 
0.9,0.9*Opportunity.Amount*SplitPercentage, 
0.99,0.99*Opportunity.Amount*SplitPercentage, 
1,Opportunity.Amount*SplitPercentage, 
0), 

IF(ISPICKVAL(Opportunity.Type,&quot;Quota&quot;),
Opportunity.Amount*SplitPercentage, 

CASE(Opportunity.Probability, 
0.25,0.5*MIN(250000,Opportunity.Amount*SplitPercentage), 
0.5,0.75*MIN(250000,Opportunity.Amount*SplitPercentage), 
0.9,0.9*Opportunity.Amount*SplitPercentage, 
0.99,0.99*Opportunity.Amount*SplitPercentage, 
1,Opportunity.Amount*SplitPercentage, 
0))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Forecasted Split Bookings (Quotas)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
