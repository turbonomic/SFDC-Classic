        <errorConditionFormula>AND(
OR(
ISCHANGED(SplitOwnerId),
ISCHANGED(SplitPercentage),
ISCHANGED(Checked_By_Finance__c)),
$UserRole.Name&lt;&gt;&quot;CFO&quot;,
NOT(CONTAINS($UserRole.Name,&quot;Finance&quot;)),
$UserRole.Name&lt;&gt;&quot;VMT SF admin&quot;)</errorConditionFormula>
