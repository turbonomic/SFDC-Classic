    <fields>
        <fullName>Discount</fullName>
    </fields>
    <fields>
        <fullName>Discount_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(PricebookEntry.UnitPrice=0,NULL, 
TEXT(ROUND((1-UnitPrice/(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c))*100,0))&amp;&quot;%&quot;)</formula>
        <label>Discount</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Discount_Percentage__c</fullName>
        <externalId>false</externalId>
        <label>Discount %</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>ListPrice</fullName>
    </fields>
    <fields>
        <fullName>List_Price_Text__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(Quote.Currency__c, 
&quot;US Dollars&quot;,&quot;$&quot;, 
&quot;UK Pounds&quot;,&quot;£&quot;, 
&quot;Euros&quot;,&quot;€&quot;, 
&quot;&quot;)&amp;

IF( 
  PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c &gt;= 1000000, 
  TEXT(FLOOR(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c / 1000000)) &amp; &quot;,&quot;, 
  &quot;&quot;) &amp; 
IF( 
  PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c &gt;= 1000, 
  RIGHT(TEXT(FLOOR(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c / 1000)), 3) &amp; &quot;,&quot;, 
  &quot;&quot;) &amp; 
RIGHT(TEXT(FLOOR(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c)), 3) &amp; &quot;.&quot; &amp; 
IF( 
  MOD(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c, 1) * 100 &lt; 10, 
  &quot;0&quot; &amp; TEXT(ROUND(MOD(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c, 1), 2) * 100), 
  TEXT(MIN(ROUND(MOD(PricebookEntry.UnitPrice*Quote.Exchange_Rate_for_Calcs__c, 1), 2) * 100, 99)) 
)</formula>
        <label>List Price</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
